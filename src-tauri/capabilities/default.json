{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "remote": {
    "urls": ["https://*.fosky.top"]
  },
  "permissions": [
    "core:default",
    "blec:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://*.fosky.top" }]
    }
  ]
}
